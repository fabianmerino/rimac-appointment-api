{
  "name": "rimac-appointment-api",
  "version": "1.0.0",
  "description": "Reto Backend Rimac - Serverless AWS Lambda",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "remove:dev": "serverless remove --stage dev",
    "remove:prod": "serverless remove --stage prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "eslint src --ext .ts --fix",
    "start:local": "serverless offline"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "typescript",
    "rimac"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.876.0",
    "@aws-sdk/client-eventbridge": "^3.876.0",
    "@aws-sdk/client-sns": "^3.876.0",
    "@aws-sdk/lib-dynamodb": "^3.876.0",
    "@types/mime-types": "^3.0.1",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "mysql2": "^3.14.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^12.0.0",
    "@eslint/js": "^9.34.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "jest": "^30.1.1",
    "serverless": "^4.18.2",
    "serverless-offline": "^14.4.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.15.0"
}
