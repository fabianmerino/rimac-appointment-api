# Environment variables for local development
# Copy this file to .env and update with your values

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# RDS Database Configuration
RDS_HOST=localhost
RDS_PORT=3306
RDS_USERNAME=admin
RDS_PASSWORD=password
RDS_DATABASE=appointments

# JWT Secret (if needed for authentication)
JWT_SECRET=your-secret-key-here

# Stage
STAGE=dev

# Other AWS Resource ARNs (will be set automatically by Serverless)
# SNS_TOPIC_ARN=
# SQS_PE_URL=
# SQS_CL_URL=
# SQS_COMPLETION_URL=
# EVENT_BUS_NAME=
# APPOINTMENTS_TABLE=
